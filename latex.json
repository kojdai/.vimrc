	"five_lemma":{
		"prefix": "five_lemma",
		"body": [
			"\\begin{center}\\label{five_lemma}",
		    "\\begin{tikzpicture}[auto,->]",
		        "%record1",
		        "\\node(M1) at (0,0){$$M_1$};",
		        "\\node(M2) at (3,0){$$M_2$};",
		        "\\node(M3) at (6,0){$$M_3$};",
		        "\\node(M4) at (9,0){$$M_4$};",
		        "\\node(M5) at (12,0){$$M_5$};",
		        "%arrow1",
		        "\\draw(M1) -- node{$$f_1$}(M2);",
		        "\\draw(M2) -- node{$$f_2$}(M3);",
		        "\\draw(M3) -- node{$$f_3$}(M4);",
		        "\\draw(M4) -- node{$$f_4$}(M5);",
		        "%record2",
		        "\\node(N1) at (0,-2){$$N_1$};",
		        "\\node(N2) at (3,-2){$$N_2$};",
		        "\\node(N3) at (6,-2){$$N_3$};",
		        "\\node(N4) at (9,-2){$$N_4$};",
		        "\\node(N5) at (12,-2){$$N_5.$};",
		        "%arrow2",
		        "\\draw(M1) -- node{$$h_1$}(N1);",
		        "\\draw(M2) -- node{$$h_2$}(N2);",
		        "\\draw(M3) -- node{$$h_3$}(N3);",
		        "\\draw(M4) -- node{$$h_4$}(N4);",
		        "\\draw(M5) -- node{$$h_5$}(N5);",
		        "%arrow3",
		        "\\draw(N1) -- node{$$g_1$}(N2);",
		        "\\draw(N2) -- node{$$g_2$}(N3);",
		        "\\draw(N3) -- node{$$g_3$}(N4);",
		        "\\draw(N4) -- node{$$g_4$}(N5);",
		    "\\end{tikzpicture}",
			"\\end{center}",
		]
	},
